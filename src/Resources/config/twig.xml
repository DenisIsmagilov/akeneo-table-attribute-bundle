<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="flagbit_table_attribute.twig.attribute_extension.class">Flagbit\Bundle\TableAttributeBundle\Twig\AttributeExtension</parameter>
    </parameters>
    <services>
        <!-- Twig extensions -->
        <service id="flagbit.twig.attribute_extension" class="%flagbit_table_attribute.twig.attribute_extension.class%">
            <argument type="service" id="pim_catalog.repository.attribute"/>
            <argument>%pim_enrich.attribute_icons%</argument>
            <argument>%pimee_enrich.attribute_icons%</argument>
            <argument>%flagbit_table_attribute.attribute_icons%</argument>
            <tag name="twig.extension"/>
        </service>
    </services>
</container>
